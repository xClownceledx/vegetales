// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model vegetales {
  id Int @id @default(autoincrement())
  nombre String
  pais String?
  precio String?
  cantidad String?
  tipofruta String?
}